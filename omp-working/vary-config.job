#SBATCH --mail-type=END
#SBATCH --mail-user=akhlaque.ak@gmail.com
module load Valgrind
alias python=python3

export OMP_NUM_THREADS=32
./kplex -g ../datasets/wiki-it.txt -k1 3 -k2 3 -q 12 > wikit-it-32.out


export OMP_NUM_THREADS=16
./kplex -g ../datasets/wiki-it.txt -k1 3 -k2 3 -q 12 > wikit-it-16.out


export OMP_NUM_THREADS=8
./kplex -g ../datasets/wiki-it.txt -k1 3 -k2 3 -q 12 > wikit-it-8.out